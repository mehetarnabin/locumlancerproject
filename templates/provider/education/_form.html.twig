{{ form_start(form, {'attr': {'id': 'addEducationForm', 'class': 'education-form needs-validation', 'novalidate': 'novalidate'}}) }}
<div class="row">
    <div class="col-12">
        {{ form_row(form.school, {'attr': {'class': 'form-control mb-2'}}) }}
    </div>
    <div class="col">
        {{ form_row(form.country, {'attr': {'class': 'form-control mb-2'}}) }}
        {{ form_row(form.state, {'attr': {'class': 'form-control mb-2'}}) }}
        {{ form_row(form.city, {'attr': {'class': 'form-control mb-2'}}) }}
    </div>

    <div class="col">
        {{ form_row(form.degree, {'attr': {'class': 'form-control mb-2'}}) }}
        {{ form_row(form.startDate, {'attr': {'class': 'form-control mb-2 js-datepicker'}}) }}
        {{ form_row(form.endDate, {'attr': {'class': 'form-control mb-2 js-datepicker'}}) }}
    </div>
</div>

<br/>
<button type="submit" class="btn btn-primary">Save</button>

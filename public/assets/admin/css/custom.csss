/* job dtail */
.job-details {
    padding: 20px;
    border-radius: 4px;
    padding-top: 5px;
    border: 1px solid var(--custom-green);
    display: grid;
    grid-template-columns: repeat(3, 1fr);
    padding-bottom: 5px;
}

.job-details .job-row:last-child {
    grid-column: span 3;
}

.table-row {
    border: 1px solid var(--custom-green);
    padding: 20px;
    border-radius: 4px;
    padding-top: 5px;
    display: grid;
    grid-template-columns: repeat(3, 1fr);
    padding-bottom: 5px;
}

.job-row {
    font-size: 15px;
    padding: 15px 0;
    border-bottom: 1px solid var(--light-yellow);
    flex: 0 0 33.333%;
    color: #252930;
}

.job-row .job-label {
    font-weight: bold;
    color: var(--primary-color);
    margin-bottom: 3px;
    font-size: 15px;
}

.job-row p {
    margin-bottom: 0;
}

/* job dtail */
.application-sticky {
    position: sticky;
    top: 75px;
    background: #fff;
    z-index: 1;
    padding: 12px 0;
    padding-bottom: 0;
}

@media (max-width: 768px) {

    .job-details,
    .table-row {
        grid-template-columns: repeat(2, 1fr);
    }

    .job-details .job-row:last-child {
        grid-column: span 2;
    }
}

@media (max-width: 480px) {

    .job-details,
    .table-row {
        grid-template-columns: repeat(1, 1fr);
    }

    .job-details .job-row:last-child {
        grid-column: span 1;
    }
}

